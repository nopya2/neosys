{{ form_start(form) }}
	<div class="form-body">
        <div class="form-group">
            {{ form_help(form.nom) }}
            <label class="control-label mb-10">Nom</label>
            {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
            <span class="help-block"> {{ form_errors(form.nom) }}</span> 
        </div>
        <div class="form-group">
            {{ form_help(form.document) }}
            <label class="control-label mb-10">Fichier</label>
            {{ form_widget(form.document, {'attr': {'class': 'dropify'}}) }}
            <span class="help-block"> {{ form_errors(form.document) }}</span> 
        </div>
	</div>
	<div class="form-actions mt-10">
		<button type="submit" class="btn btn-success  btn-icon left-iocn mr-10"><i class="fa fa-upload"></i>{{ button_label|default('Importer') }}</button>
		{# <button type="button" class="btn btn-default">Cancel</button> #}
	</div>
{{ form_end(form) }}
