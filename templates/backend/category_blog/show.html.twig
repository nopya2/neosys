{% extends 'base.html.twig' %}

{% block title %}CategoryBlog{% endblock %}

{% block body %}
    <h1>CategoryBlog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category_blog.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ category_blog.titre }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ category_blog.createdAt ? category_blog.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ category_blog.updatedAt ? category_blog.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('category_blog_index') }}">back to list</a>

    <a href="{{ path('category_blog_edit', {'id': category_blog.id}) }}">edit</a>

    {{ include('category_blog/_delete_form.html.twig') }}
{% endblock %}
