
<!DOCTYPE html>
<html lang="fr-Fr" dir="ltr">
<meta http-equiv="content-type" content="text/html;charset=utf-8" />
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="canonical" href="neosystechnologie.ga">
    <base  />
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <title>{% block titre %}{% endblock %}</title>
    {# css files #}
    {{ include('portail/include/_css.html.twig', {'page': page}) }}
    {# js files #}
    {{ include('portail/include/_js.html.twig', {'page': page}) }}
	<meta property="og:title" content="Neosys Technologie" />
	<meta property="og:type" content="website" />
	<meta property="og:url" content="http://neosystechnologie.ga" />
	<meta property="og:site_name" content="Neosys Technologie" />
	<meta name="twitter:card" content="summary" />
	<meta name="twitter:site" content="Neosys Technologie" />
    </head>
    <body class="site helix-ultimate com-sppagebuilder view-page layout-default task-none itemid-123 en-gb ltr sticky-header layout-fluid offcanvas-init offcanvs-position-right">
        <div id="loading">
            <div id="loading-center">
                <div id="loading-center-absolute">
                    <div class="object" id="object_one"></div>
                    <div class="object" id="object_two"></div>
                    <div class="object" id="object_three"></div>
                    <div class="object" id="object_four"></div>
                </div>
            </div>
        </div>
        <div class="body-wrapper">
            <div class="body-innerwrapper">
                {% if page == 'home' %}
                <div class="transparent-header">
                    <!-- Top Bar -->
                    {{ render(controller(
                        'App\\Controller\\PortailController::topBar'
                    )) }}
                    <!-- Header -->
                    {{ include('portail/include/_header.html.twig') }}
                </div>
                {% else %}
                <!-- Top Bar -->
                {{ render(controller(
                    'App\\Controller\\PortailController::topBar'
                )) }}
                <!-- Header -->
                {{ include('portail/include/_header.html.twig', {'page': page}) }}
                {% endif %}
                
                <section id="sp-section-1">
                    <div class="row">
                        <div id="sp-title" class="col-lg-12 ">
                            <div class="sp-column "></div>
                        </div>
                    </div>
                </section>
                <section id="sp-main-body">
                    <div class="row">
                        <main id="sp-component" class="col-lg-12 " role="main">
                            <div class="sp-column ">
                                <div id="system-message-container"></div>
                                {% block article_details %}{% endblock %}
                                
                                <div id="sp-page-builder" class="sp-page-builder  page-4">
                                    <div class="page-content">
                                        {% block page_content %}{% endblock %}

                                        
                                    </div>
                                </div>
                            </div>
                        </main>
                    </div>
                </section>

                <!-- bottom -->
                {{ render(controller(
                    'App\\Controller\\PortailController::bottom',
                    {
                        'max' : 3
                    }
                )) }}
                <!-- ./bottom -->

                <!-- footer -->
                {{ render(controller(
                    'App\\Controller\\PortailController::footer'
                )) }}
                <!-- ./footer -->
            </div>
        </div>
        
        <!-- Off Canvas Menu -->
        <div class="offcanvas-overlay"></div>
        <div class="offcanvas-menu">
            <a href="#" class="close-offcanvas"><span class="fa fa-remove"></span></a>
            <div class="offcanvas-inner">
                <div class="sp-module ">
                    <div class="sp-module-content">
                        <ul class="menu">
                            <li class="item-101 default active menu-deeper menu-parent">
                                <a href="#" >ACCUEIL</a>
                            </li>
                            <li class="item-104 menu-deeper menu-parent">
                                <a href="#" >A PROPOS</a>
                            </li>
                            <li class="item-138 menu-deeper menu-parent">
                                <a href="#" >SERVICES</a>
                            </li>
                            <li class="item-146 menu-deeper menu-parent">
                                <a href="#" >PROJETS</a>
                            </li>
                            <li class="item-155 menu-deeper menu-parent">
                                <a href="#" >EQUIPE</a>
                            </li>
                            <li class="item-133 menu-deeper menu-parent">
                                <a href="#" >PLUS<span class="menu-toggler"></span></a>
                                <ul class="menu-child">
                                    <li class="item-134">
                                        <a href="#" >ACTUALITES</a>
                                    </li>
                                    <li class="item-135">
                                        <a href="#" >EVENEMENTS</a>
                                    </li>
                                    <li class="item-135">
                                        <a href="#" >FORMATIONS</a>
                                    </li>
                                    <li class="item-135">
                                        <a href="#" >CONTACTS</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>   
    
    
        <!-- Go to top -->
        <a href="#" class="sp-scroll-up" aria-label="Scroll Up"><span class="fa fa-chevron-up" aria-hidden="true"></span></a>
    </body>
</html>